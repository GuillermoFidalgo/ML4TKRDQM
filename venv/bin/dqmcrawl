#!/uscms_data/d3/gfidalgo/ML4WTRK/CMSSW_10_2_10/src/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'dqmcrawlr==0.7.1','console_scripts','dqmcrawl'
__requires__ = 'dqmcrawlr==0.7.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('dqmcrawlr==0.7.1', 'console_scripts', 'dqmcrawl')()
    )
