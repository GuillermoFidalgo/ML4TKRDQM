#!/uscms_data/d3/gfidalgo/ML4WTRK/CMSSW_10_2_10/src/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'runregcrawlr==0.5.1','console_scripts','runregcrawl'
__requires__ = 'runregcrawlr==0.5.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('runregcrawlr==0.5.1', 'console_scripts', 'runregcrawl')()
    )
