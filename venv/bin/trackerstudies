#!/uscms_data/d3/gfidalgo/ML4WTRK/CMSSW_10_2_10/src/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'trackerstudies==0.1.1','console_scripts','trackerstudies'
__requires__ = 'trackerstudies==0.1.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('trackerstudies==0.1.1', 'console_scripts', 'trackerstudies')()
    )
